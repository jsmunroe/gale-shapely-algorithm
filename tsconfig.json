{
    "compilerOptions": {
        "jsx": "react-jsx",
        "outDir": "./built",
        "sourceMap": true,
        "allowJs": true,
        "lib": [ "es6" ],
        "target": "es5",
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
    },
    "include": ["./src/**/*"],
    "exclude": ["**/*.test.*"],
}